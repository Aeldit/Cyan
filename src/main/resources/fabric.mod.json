{
  "schemaVersion": 1,
  "id": "cyan",
  "version": "${version}",
  "name": "Cyan",
  "description": "This mod allows you to save global locations, teleport to your bed / respawn anchor or to the surface, and some more !",
  "authors": [
    "Aeldit"
  ],
  "contact": {
    "homepage": "https://modrinth.com/user/Aeldit",
    "sources": "https://github.com/Aeldit/Cyan",
    "issues": "https://github.com/Aeldit/Cyan/issues"
  },
  "license": "LGPL-3.0 license",
  "icon": "assets/cyan/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      "fr.aeldit.cyan.CyanServerCore"
    ],
    "client": [
      "fr.aeldit.cyan.CyanClientCore"
    ],
    "modmenu": [
      "fr.aeldit.cyan.config.ModMenuApiImpl"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${min} <=${max}",
    "java": ">=${java_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/PcYPpqzhKS"
      }
    }
  }
}
